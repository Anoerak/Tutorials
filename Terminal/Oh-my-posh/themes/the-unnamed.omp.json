{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#ff5e5e",
          "style": "plain",
          "template": "\udb80\uddd9 "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "project",
          "foreground": "#5EADF2",
          "style": "plain",
          "template": " {{if .Version}}\uf072 Project: {{.Name}} \uf487 {{.Version}}, {{end}}"
        },
        {
          "type": "node",
          "foreground": "#44FFD2",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "node \ue718 {{ .Full }}, "
        },
        {
          "type": "npm",
          "foreground": "#FE4A49",
          "style": "plain",
          "template": "npm \ue71e {{.Full}} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#33FFB7",
          "style": "plain",
          "template": " \uf51e Stack:"
        },
        {
          "type": "php",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "{{if .Full}} PHP  {{ .Full }}{{end}} |"
        },
        {
          "type": "react",
          "foreground": "#81a1c1",
          "style": "plain",
          "template": "{{if .Full}} React  {{ .Full }}{{end}} |"
        },
        {
          "type": "swift",
          "foreground": "#ffffff",
          "style": "plain",
          "template": "{{if .Full}} Swift  {{ .Full }}{{end}} |"
        },
        {
          "type": "docker",
          "foreground": "#0B59E7",
          "style": "plain",
          "template": "{{if .Context}} Docker  {{ .Context }}{{end}}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "foreground": "#15C2CB",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " \ue5fb {{ .Path }} "
        },
        {
          "type": "git",
          "foreground": "#F141A8",
          "properties": {
            "branch_icon": "\ue0a0 "
          },
          "style": "plain",
          "template": "<#F3EFF5>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#FFE45E",
          "style": "plain",
          "template": "\u276f "
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
